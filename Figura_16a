ggplot(dados) +
  aes(
    x = `1 - Foi elaborado um comparativo sobre os custos,
    entre sistemas sobre demanda e módulos de sistemas ERP's antes da contratação?`
  ) +
  geom_histogram(bins = 10L, fill = "#2351A3") +
  labs(x = ".", y = ".") +
  coord_flip() +
  theme_bw() +
  facet_wrap(vars(`Qual sua função?`))
